    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'stripe-update'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=standalone",
                    "--package=stripe-update"
                ],
                "filter": {
                    "name": "standalone",
                    "kind": "bin"
                }
            },
            "args": [
                "build-data",
                "-c=./run_config.toml"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'cluster worker'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=cluster",
                    "--package=stripe-update"
                ],
                "filter": {
                    "name": "cluster",
                    "kind": "bin"
                }
            },
            "args": [
                "worker",
                "-c=./run_config.toml",
                "-i=1"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'cluster coordinator kill-all'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=cluster",
                    "--package=stripe-update"
                ],
                "filter": {
                    "name": "cluster",
                    "kind": "bin"
                }
            },
            "args": [
                "coordinator",
                "-c=./run_config.toml",
                "kill-all"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'cluster coordinator build-data'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=cluster",
                    "--package=stripe-update"
                ],
                "filter": {
                    "name": "cluster",
                    "kind": "bin"
                }
            },
            "args": [
                "coordinator",
                "-c=./run_config.toml",
                "build-data"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'cluster coordinator purge'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=cluster",
                    "--package=stripe-update"
                ],
                "filter": {
                    "name": "cluster",
                    "kind": "bin"
                }
            },
            "args": [
                "coordinator",
                "-c=./run_config.toml",
                "purge"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'cluster coordinator bench-update'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=cluster",
                    "--package=stripe-update"
                ],
                "filter": {
                    "name": "cluster",
                    "kind": "bin"
                }
            },
            "args": [
                "coordinator",
                "-c=./run_config.toml",
                "bench-update"
            ],
            "cwd": "${workspaceFolder}"
        }
    ]
}