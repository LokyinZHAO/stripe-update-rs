    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'stripe-update'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=stripe-update"
                ],
                "filter": {
                    "name": "stripe-update",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'stripe-update'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=stripe-update",
                    "--package=stripe-update"
                ],
                "filter": {
                    "name": "stripe-update",
                    "kind": "bin"
                }
            },
            "args": [
                "benchmark",
                "-m=baseline",
                "-c=./run_config.toml"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'stripe-update'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=stripe-update",
                    "--package=stripe-update"
                ],
                "filter": {
                    "name": "stripe-update",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}